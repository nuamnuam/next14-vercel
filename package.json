{
  "name": "Arzinja-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "find:unused": "next-unused",
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "prettier:fix": "prettier --write './src/**/*.{js,jsx,ts,tsx,json,md}'",
    "prepare": "husky install",
    "generate:icons": "npx @svgr/cli --typescript --out-dir ./src/components/Icons --ignore-existing -- ./src/assets/icons",
    "generate:tailwind": "node ./bin/generate-tailwind-theme",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "generate-lang-types": "node ./src/language/generateLangTypes.js"
  },
  "next-unused": {
    "alias": {},
    "include": [],
    "exclude": [],
    "entrypoints": [
      "src/pages",
      "src/components",
      "src/assets",
      "src/constants",
      "src/hooks",
      "src/requests",
      "src/store",
      "src/styles",
      "src/utils",
      "src/types"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier . --write --ignore-pattern 'src/language/'",
      "eslint . --fix --ignore-pattern 'public/'",
      "git add"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hassanmojab/react-modern-calendar-datepicker": "^3.1.7",
    "@hookform/resolvers": "^2.9.7",
    "@mui/lab": "^5.0.0-alpha.114",
    "@mui/material": "^5.15.16",
    "@persian-tools/persian-tools": "^3.5.2",
    "@tanstack/react-query": "^5.51.1",
    "@types/mime": "^4.0.0",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "classnames": "^2.3.2",
    "date-fns": "^2.28.0",
    "firebase": "^10.12.2",
    "jalali-moment": "^3.3.11",
    "mapbox-gl": "^3.0.1",
    "next": "^14.2.3",
    "next-pwa": "^5.6.0",
    "next-qrcode": "^2.5.0",
    "nextjs-progressbar": "^0.0.14",
    "pusher-js": "^8.3.0",
    "qs": "^6.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^7.43.0",
    "react-icomoon": "^2.5.7",
    "react-intl": "^6.6.6",
    "react-loading-skeleton": "^3.2.0",
    "react-map-gl": "^7.1.7",
    "react-modern-calendar-datepicker": "^3.1.6",
    "react-otp-input": "^2.4.0",
    "react-qr-code": "^2.0.11",
    "react-toastify": "^10.0.5",
    "react-use": "^17.4.0",
    "react-webcam": "^7.0.1",
    "recharts": "^2.5.0",
    "sass": "^1.58.1",
    "swiper": "^11.1.1",
    "yup": "^0.32.11",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@marker.io/browser": "^0.19.0",
    "@next/bundle-analyzer": "^12.2.0",
    "@storybook/addon-essentials": "^7.0.0-beta.26",
    "@storybook/addon-interactions": "^7.0.0-beta.26",
    "@storybook/addon-links": "^7.0.0-beta.26",
    "@storybook/blocks": "^7.0.0-beta.26",
    "@storybook/nextjs": "^7.0.0-beta.26",
    "@storybook/react": "^7.0.0-beta.26",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/cli": "^6.2.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.2",
    "@types/node": "18.0.1",
    "@types/qs": "^6.9.7",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.0.1",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^8.9.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next-unused": "^0.0.6",
    "postcss": "^8.4.14",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "pretty-quick": "^3.1.3",
    "storybook": "^7.0.0-beta.26",
    "tailwindcss": "^3.1.4",
    "typescript": "*",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
